let person = {
  title: 'Duke',
  name: 'Nukem',
  age: 33
};

// Expected output:
// [['title', 'Duke'], ['name', 'Nukem'], ['age', 33]]

//using mdn documentation i found .entries, usign this you can create nested arrays out of an obkject

let nestedPerson = Object.entries(person);
console.log(nestedPerson);

//[ [ 'title', 'Duke' ], [ 'name', 'Nukem' ], [ 'age', 33 ] ]
